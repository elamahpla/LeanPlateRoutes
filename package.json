{
  "name": "super-lean-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass src/sass/main.scss css/style.css -w",
    "compile:sass": "node-sass src/sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "start": "webpack-dev-server --config ./webpack.config.js src/sass/main.scss -w",
    "build:dev": "webpack",
    "build:prod": "webpack -p --env production"
  },
  "keywords": [],
  "author": "Joey Robinson <joeyrobinsondev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "postcss": "^6.0.22",
    "postcss-cli": "^6.1.2",
    "react-hot-loader": "^3.1.3",
    "webpack": "^4.33.0",
    "webpack-dev-server": "^3.6.0"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.1.1",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2"
  }
}
